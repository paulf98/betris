<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<businessObjectModel xmlns="http://documentation.bonitasoft.com/bdm-xml-schema/1.0" modelVersion="1.0" productVersion="7.15.0">
    <businessObjects>
        <businessObject qualifiedName="com.company.model.Customer">
            <fields>
                <field type="STRING" length="255" name="customerId" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="customerName" nullable="true" collection="false"/>
                <relationField type="AGGREGATION" reference="com.company.model.OrderItems" fetchType="LAZY" name="customerOrders" nullable="true" collection="true"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="com.company.model.Item">
            <fields>
                <field type="STRING" length="255" name="itemId" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="itemName" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="itemDescription" nullable="true" collection="false"/>
                <field type="INTEGER" length="255" name="itemAmount" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="itemAvailability" nullable="true" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="com.company.model.OrderItems">
            <description>Reflects an order.</description>
            <fields>
                <field type="STRING" length="255" name="orderId" nullable="true" collection="false"/>
                <relationField type="AGGREGATION" reference="com.company.model.Item" fetchType="LAZY" name="orderItems" nullable="true" collection="true"/>
                <field type="OFFSETDATETIME" length="255" name="orderDate" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="orderStatus" nullable="true" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
    </businessObjects>
</businessObjectModel>
