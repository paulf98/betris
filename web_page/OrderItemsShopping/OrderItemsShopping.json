        "\t$data.orderItems.orderItems = $data.orderItems_orderItems;",
        "\t$data.orderItems.orderCustomer = $data.orderItems_orderCustomer;",
        "\treturn {",
        "\t\t//map orderItems variable to expected task contract input",
        "\t\torderItemsInput: {",
        "\t\t\torderItems: $data.orderItems.orderItems.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null",
        "\t\t\t})),",
        "\t\t\torderDate: $data.orderItems.orderDate !== undefined ? $data.orderItems.orderDate : null,",
